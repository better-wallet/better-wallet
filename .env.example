# PostgreSQL Configuration
POSTGRES_DSN=postgres://user:pass@localhost:5432/bw?sslmode=disable

# App-Level Authentication (required for all API requests)
APP_ID=your-app-id
APP_SECRET=your-app-secret

# User Authentication Configuration
AUTH_KIND=oidc
AUTH_ISSUER=https://accounts.example.com
AUTH_AUDIENCE=bw-app
AUTH_JWKS_URI=https://accounts.example.com/.well-known/jwks.json

# Key Execution Backend
EXECUTION_BACKEND=kms
KMS_KEY_ID=kms/main-key
TEE_ATTESTATION_REQUIRED=false

# EVM RPC Endpoint
RPC_ENDPOINT=https://eth-mainnet.g.alchemy.com/v2/xxx

# Rate Limiting (optional)
RATE_LIMIT_QPS=100

# Server Configuration
PORT=8080
